<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f3f1f2">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="24.75dp"
            android:text="@string/register_titleText"
            android:textColor="#2a2a2a"
            android:textSize="16.50dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="251.50dp"
            android:layout_marginTop="14.50dp"
            android:background="#ffffff"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView1"
            android:orientation="horizontal" />

        <ImageView
            android:id="@+id/image_completeAgreement"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_round_off"
            android:layout_marginLeft="21.25dp"
            android:layout_marginTop="15.25dp"
            app:layout_constraintTop_toTopOf="@+id/linearLayout"
            app:layout_constraintLeft_toLeftOf="@+id/linearLayout"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agreeTitleText"
            android:textStyle="bold"
            android:textSize="14dp"
            android:textColor="#1e1e1e"
            android:layout_marginLeft="10.25dp"
            android:layout_marginTop="14.25dp"
            app:layout_constraintTop_toTopOf="@+id/linearLayout"
            app:layout_constraintLeft_toRightOf="@+id/image_completeAgreement"/>

        <!---->

        <View
            android:id="@+id/view"
            android:layout_width="329.50dp"
            android:layout_height="0.25dp"
            android:background="#cdcdcd"
            android:layout_marginTop="13.50dp"
            android:layout_centerInParent="true"
            app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
            app:layout_constraintTop_toBottomOf="@+id/image_completeAgreement"
            />

        <!---->

        <ImageView
            android:id="@+id/check1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/check_box"
            android:layout_marginTop="15.25dp"
            app:layout_constraintLeft_toLeftOf="@+id/image_completeAgreement"
            app:layout_constraintTop_toBottomOf="@+id/view" />

        <ImageView
            android:id="@+id/image_termsConditions1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_off"
            app:layout_constraintLeft_toLeftOf="@+id/check1"
            app:layout_constraintTop_toTopOf="@+id/check1"/>

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10.25dp"
            android:layout_marginTop="14.75dp"
            android:text="@string/register_agree1Text"
            android:textColor="#1e1e1e"
            android:textSize="12dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toRightOf="@+id/check1"
            app:layout_constraintTop_toBottomOf="@+id/view" />

        <ImageView
            android:id="@+id/viewAll_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/view_all_off"
            android:layout_marginRight="21.25dp"
            android:layout_marginTop="17.25dp"
            app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintRight_toRightOf="@+id/linearLayout"/>

        <!---->
        <ImageView
            android:id="@+id/check2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/check_box"
            android:layout_marginTop="13.50dp"
            app:layout_constraintLeft_toLeftOf="@+id/image_completeAgreement"
            app:layout_constraintTop_toBottomOf="@+id/check1" />


        <ImageView android:id="@+id/image_termsConditions2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_off"
            app:layout_constraintLeft_toLeftOf="@+id/check2"
            app:layout_constraintTop_toTopOf="@+id/check2"/>

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agree2Text"
            android:textColor="#1e1e1e"
            android:layout_marginTop="15.25dp"
            android:textSize="12dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="@+id/textView5"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />

        <ImageView
            android:id="@+id/viewAll_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/view_all_off"
            android:layout_marginTop="17.75dp"
            app:layout_constraintTop_toBottomOf="@+id/viewAll_1"
            app:layout_constraintLeft_toLeftOf="@+id/viewAll_1"/>

        <!---->
        <ImageView
            android:id="@+id/check3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/check_box"
            android:layout_marginTop="13.50dp"
            app:layout_constraintLeft_toLeftOf="@+id/image_completeAgreement"
            app:layout_constraintTop_toBottomOf="@+id/check2" />


        <ImageView
            android:id="@+id/image_termsConditions3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_off"
            app:layout_constraintLeft_toLeftOf="@+id/check3"
            app:layout_constraintTop_toTopOf="@+id/check3"/>

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agree3Text"
            android:textColor="#1e1e1e"
            android:layout_marginTop="14.25dp"
            android:textSize="12dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/textView6" />

        <ImageView
            android:id="@+id/viewAll_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/view_all_off"
            android:layout_marginTop="17.75dp"
            app:layout_constraintTop_toBottomOf="@+id/viewAll_2"
            app:layout_constraintLeft_toLeftOf="@+id/viewAll_2"/>


        <!---->
        <ImageView
            android:id="@+id/check4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/check_box"
            android:layout_marginTop="13.50dp"
            app:layout_constraintLeft_toLeftOf="@+id/image_completeAgreement"
            app:layout_constraintTop_toBottomOf="@+id/check3" />


        <ImageView
            android:id="@+id/image_termsConditions4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_off"
            app:layout_constraintLeft_toLeftOf="@+id/check4"
            app:layout_constraintTop_toTopOf="@+id/check4"/>


        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agree4Text"
            android:textColor="#1e1e1e"
            android:layout_marginTop="14.25dp"
            android:textSize="12dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <ImageView
            android:id="@+id/viewAll_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/view_all_off"
            android:layout_marginTop="18.75dp"
            app:layout_constraintTop_toBottomOf="@+id/viewAll_3"
            app:layout_constraintLeft_toLeftOf="@+id/viewAll_3"/>


        <!---->
        <ImageView
            android:id="@+id/check5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/check_box"
            android:layout_marginTop="13.50dp"
            app:layout_constraintLeft_toLeftOf="@+id/image_completeAgreement"
            app:layout_constraintTop_toBottomOf="@+id/check4" />


        <ImageView
            android:id="@+id/image_termsConditions5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_off"
            app:layout_constraintLeft_toLeftOf="@+id/check5"
            app:layout_constraintTop_toTopOf="@+id/check5"/>


        <TextView
            android:id="@+id/textVie9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agree5Text"
            android:textColor="#1e1e1e"
            android:layout_marginTop="14.25dp"
            android:textSize="12dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />


        <ImageView
            android:id="@+id/viewAll_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/view_all_off"
            android:layout_marginTop="17.75dp"
            app:layout_constraintTop_toBottomOf="@+id/viewAll_4"
            app:layout_constraintLeft_toLeftOf="@+id/viewAll_4"/>


        <!---->
        <ImageView
            android:id="@+id/check6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/check_box"
            android:layout_marginTop="13.50dp"
            app:layout_constraintLeft_toLeftOf="@+id/image_completeAgreement"
            app:layout_constraintTop_toBottomOf="@+id/check5" />


        <ImageView
            android:id="@+id/image_termsConditions6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/agree_off"
            app:layout_constraintLeft_toLeftOf="@+id/check6"
            app:layout_constraintTop_toTopOf="@+id/check6"/>

        <TextView
            android:id="@+id/textVie10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agree6Text"
            android:textColor="#1e1e1e"
            android:layout_marginTop="14.25dp"
            android:textSize="12dp"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="@+id/textVie9"
            app:layout_constraintTop_toBottomOf="@+id/textVie9" />

        <ImageView
            android:id="@+id/viewAll_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/view_all_off"
            android:layout_marginTop="17.75dp"
            app:layout_constraintTop_toBottomOf="@+id/viewAll_5"
            app:layout_constraintLeft_toLeftOf="@+id/viewAll_5"/>


        <!---->

        <ImageView
            android:id="@+id/imageView_register"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/join_btn"
            android:layout_marginTop="22.75dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:scaleType="fitXY"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout"/>

        <com.kakao.usermgmt.LoginButton
            android:id="@+id/imageView_kakao"
            android:layout_width="match_parent"
            android:layout_height="41.50dp"
            android:layout_marginTop="8.50dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView_register"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/register_agree7Text"
            android:textStyle="bold"
            android:layout_marginTop="9.50dp"
            android:textSize="8.50dp"
            app:layout_constraintLeft_toLeftOf="@+id/linearLayout"
            app:layout_constraintTop_toBottomOf="@+id/imageView_kakao"/>

    </android.support.constraint.ConstraintLayout>


</layout>